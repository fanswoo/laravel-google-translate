{
    "name": "fanswoo/laravel-google-translate",
    "description": "laravel google translate",
    "type": "library",
    "require": {
        "php": "^8.4",
        "laravel/framework": "^11.0",
        "google/auth": "^1.47"
    },
    "keywords": [
        "laravel",
        "google translate",
        "fanswoo"
    ],
    "autoload": {
        "psr-4": {
            "FF\\GoogleTranslate\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/fanswoo/laravel-google-translate",
        "source": "https://github.com/fanswoo/laravel-google-translate"
    },
    "authors": [
        {
            "name": "Yi Yang",
            "email": "sacriley@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "FF\\GoogleTranslate\\GoogleTranslateProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12.2"
    }
}
